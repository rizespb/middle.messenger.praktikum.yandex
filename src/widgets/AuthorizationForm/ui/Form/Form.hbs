<form class={{classes.form}} >
	<div class={{classes.inputs}} >
		{{#each inputs}}
			{{{this}}}
		{{/each}}

		{{#if passwordsError}}
			<div class="{{classes.passwordsError}}">
				{{{passwordsError}}}
			</div>
		{{/if}}
	</div>

	<div class={{classes.buttons}} >
		{{#each buttons}}
			{{{this}}}
		{{/each}}
	</div>
</form>
